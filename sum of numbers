import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n,sum=0;
         do
        {
       n=s.nextInt();
        sum+=n;  
        }
        while(n!=-1);
        {
            System.out.println(sum+1);
    }
    }
}
