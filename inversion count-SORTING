import java.util.*;
class InversionCount
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int m=s.nextInt();
int a[]=new int[m];
for(int i=0;i<m;i++)
{
 a[i]=s.nextInt();
}
int c=0;
for(int i=0;i<m-1;i++)
{
for(int j=i+1;j<m;j++)
{
if(a[i]>a[j])
{
c++;
}
}
}
System.out.println(c);
}
}
