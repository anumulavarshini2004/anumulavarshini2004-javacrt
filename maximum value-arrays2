import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
         int n = s.nextInt();
         int max_value = Integer.MIN_VALUE;
        for (int i = 0; i < n; i++) {
            int value = s.nextInt();
            if (value > max_value) {
                max_value = value;
            }
        }
    System.out.println(max_value);
}
} 
