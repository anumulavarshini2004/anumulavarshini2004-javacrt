import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
        int n;
        n=s.nextInt();
        int a=7;
        System.out.print(a+" ");
        for(int i=0;i<n-1;i++)
        {
            if(i%2==0)
                a=a-2;
            else
                a=a+3;
            System.out.print(a+" ");
        }
    }
}
