import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int min,max;
    Scanner s=new Scanner(System.in);
        min=s.nextInt();
        max=s.nextInt();
    int sum=0;
    int i=min;
    while(i<=max){
        sum=sum+i;
        i++;
    }
    System.out.println(sum);
        
    }
}
